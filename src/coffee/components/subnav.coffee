((OJ) ->    subNav = (owner, props) ->    defaults:      active: ''      menu: [        {          name: ''          href: ''        }      ]      OJ.extend defaults, props      ret = OJ.component(owner, 'sidebar-subnav')    ul = ret.ul  attr:      class: 'animated fadeIn'    OJ.each defaults.menu, (val, key) ->      if val.name is defaults.active        li = ul.li attr:          class: 'active'      else        li = ul.li      li.a attr:        href: val.href        .text val.name      return        OJ.components.register 'subnav', subNav    OJ.components.members.push 'subnav'      )  (if (typeof global isnt 'undefined' and global) then global else if (typeof window isnt 'undefined') then window else this).OJ)