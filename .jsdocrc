{
	"plugins": [ "node_modules/grunt-jsdoc/node_modules/jsdoc/plugins/markdown" ],
	  "markdown": {
		"parser": "gfm",
		"hardwrap": true,
		"githubRepoName": "oj",
		"githubRepoOwner": "somecallmechief"
	  },
	  "templates" : {
		"cleverLinks"     : true,
		"monospaceLinks"  : false,
		"default"         : {
			"outputSourceFiles" : true
		},
		"systemName"      : "OJ",
		"footer"          : "",
		"copyright"       : "OJ is Public Domain software. Please take generously.",
		"navType"         : "vertical",
		"theme"           : "cosmo",
		"linenums"        : true,
		"collapseSymbols" : false,
		"inverseNav"      : true
	},
	"opts": {
		"lenient": "true",
		"recurse": "true",
		"template": "./doc_template/template"
	},
	"tags": {
        "allowUnknownTags": true
    }
	

}