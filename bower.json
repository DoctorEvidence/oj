  "dependencies": {
    "jquery": ">= 2.1.0",
    "bootstrap": ">= 3.1.1",
    "jquery-ui": ">= 1.10.4",
    "modernizr": ">= 2.7.2",
    "less.js": ">= 1.7.0",
    "google-code-prettify": ">= 1.0.1",
    "qunit": ">= 1.14.0",
    "q": "~1.0.1",
    "ajaxify": "https://github.com/arvgta/ajaxify.git#~4.6.4",
    "SyntaxHighlighter": ">= 3.0.83",
    "lodash": ">= 2.4.1",
    "Faker": ">= 0.7.0",
    "setimmediate": ">= 1.0.1",
    "localforage": "~0.2.1",
    "xregexp": "~2.0.0",
    "thindom": "~0.0.2",
    "mousetrap": "~1.4.6",
