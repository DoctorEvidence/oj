            var buildMode = grunt.config('buildMode');
            grunt.log.writeln('Starting JS for: ' + buildMode + '.');
            if(buildMode) {
                
                var jsFiles = grunt.file.expand(grunt.config('tmpHtml.' + buildMode + '.jsFiles'));
                grunt.log.writeln('Starting JS files');
                if(jsFiles) {
                    for(var i = 0, len = jsFiles.length; i < len; i += 1) {
                        print('        <script type="text/javascript" src="../' + jsFiles[i] + '"></script>\n');
                    }
                }
            }
