    <head class="x-border-box x-strict">
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title><%  
            var buildMode = grunt.config('buildMode');
            if(buildMode) {
                grunt.log.writeln('Starting title for buildMode: ' + buildMode + '.');
                print(grunt.config('tmpHtml.' + buildMode + '.title'));
            }
           %></title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width">

        <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->

        <%
            var buildMode = grunt.config('buildMode');
            if(buildMode) {
                grunt.log.writeln('Starting CSS for buildMode: ' + buildMode + '.');
                                
                var cssFiles = grunt.file.expand(grunt.config('tmpHtml.' + buildMode + '.cssFiles'));
                if(cssFiles) {
                    for(var i = 0, len = cssFiles.length; i < len; i += 1) {
                        print('        <link rel="stylesheet" href=""../' + cssFiles[i] + '" />\n');
                    }
                }
            }
        %>
                
        <link rel="stylesheet" href="//extjs.cachefly.net/ext-4.1.1a-gpl/resources/css/ext-all-scoped.css" type="text/css">
        
    </head>
