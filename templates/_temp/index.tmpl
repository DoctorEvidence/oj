<!DOCTYPE html manifest="<%
    var releaseMode = grunt.config('releaseMode');
    if(releaseMode === 'dev') {
        print('release/Dev.appcache');
    } else {
        print('release/Main.appcache');
    }
%>">
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
    <head class="x-border-box x-strict">
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title><%  
            var releaseMode = grunt.config('releaseMode');
            if(releaseMode) {
                grunt.log.writeln('Starting title for releaseMode: ' + releaseMode + '.');
                print(grunt.config('tmpHtml.' + releaseMode + '.title'));
            }
           %></title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width">

        <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->

        <%
            print('        <link rel="stylesheet" href="../release/vendor.min.css" />\n');
            var releaseMode = grunt.config('releaseMode');
            if(releaseMode) {
                grunt.log.writeln('Starting CSS for releaseMode: ' + releaseMode + '.');
                                
                var cssFiles = grunt.file.expand(grunt.config('tmpHtml.' + releaseMode + '.cssFiles'));
                if(cssFiles) {
                    for(var i = 0, len = cssFiles.length; i < len; i += 1) {
                        print('        <link rel="stylesheet" href="../' + cssFiles[i] + '" />\n');
                    }
                }
            }
        %>
                
        <link rel="stylesheet" href="//extjs.cachefly.net/ext-4.1.1a-gpl/resources/css/ext-all-scoped.css" type="text/css">
        
    </head>
    <body class="x-body x-webkit x-chrome x-reset">
        <!--[if lte IE 9]>
            <p class="chromeframe">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">activate Google Chrome Frame</a> to improve your experience.</p>
        <![endif]-->

      


        

        <%
            print('        <script type="text/javascript" src="//extjs.cachefly.net/ext-4.1.1a-gpl/ext-all-debug.js"></script>\n');
            print('        <script type="text/javascript" src="//extjs.cachefly.net/ext-4.1.1a-gpl/examples/ux/CheckColumn.js"></script>\n');
            print('        <script src="//ajax.aspnetcdn.com/ajax/jQuery/jquery-2.0.2.min.js"></script>\n');
            print('        <script src="//cdnjs.cloudflare.com/ajax/libs/q.js/0.9.2/q.min.js"></script>\n');
            print('        <script src="../release/vendor.min.js"></script>\n');

            var releaseMode = grunt.config('releaseMode');
            grunt.log.writeln('Starting JS for releaseMode: ' + releaseMode + '.');
            if(releaseMode) {
                
                var jsFiles = grunt.file.expand(grunt.config('tmpHtml.' + releaseMode + '.jsFiles'));
                if(jsFiles) {
                    for(var i = 0, len = jsFiles.length; i < len; i += 1) {
                        print('        <script type="text/javascript" src="../' + jsFiles[i] + '"></script>\n');
                    }
                }
            }
        %>



    </body>
</html>
